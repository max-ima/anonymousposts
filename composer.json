	"type": "phpbb-extension",
	"description": "Post anonymously in phpbb 3.2",
	"homepage": "https://github.com/toxyy/anonymousposts",
	"version": "0.3.0",
	"time": "2018-10-03",
	"keywords": ["phpbb", "extension", "anonymous", "anonymous posts"],
	"license": "GPL-2.0",
	"authors": [{
			"name": "toxyy",
			"email": "thrashtek@yahoo.com",
			"homepage": "https://github.com/toxyy/",
			"role": "Developer"
		}],
	"require": {
		"php": ">=7",
                "marttiphpbb/phpbb-ext-grouptempvars": "0.2.0"
	},
	"extra": {
		"display-name": "Anonymous Posts",
		"soft-require": {
			"phpbb/phpbb": ">=3.2,<3.3"
		},
		"version-check": {
			"host": "toxyy.github.io",
